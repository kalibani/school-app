<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">

    <title></title>
  </head>
  <body>
  <section>
    <div class="detail">
      <div class="table_data col-md-12">
        <table class="table table-hover">
           <thead>
             <tr>
               <th>No</th>
               <th>Username</th>
               <th>Password</th>
               <th>Role</th>
               <th>Secret Key</th>
             </tr>
           </thead>
           <tbody>
             <% data_users.forEach((user, index)=>{%>
              <tr>
                <td> <%= index+1 %> </td>
                <td> <%= user.username %> </td>
                <td> <%= user.password %> </td>
                <td> <%= user.role %> </td>
                <td> <%= user.secret %> </td>
              </tr>
            <%})%>
           </tbody>
         </table>
      </div>
    </div>
  </section>
</body>
</html>
