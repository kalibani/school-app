<!DOCTYPE html>
<html>
  <%- include partial/head %>
  <body>
    <h1>Halaman Subjects</h1>
    <a href="/">Back to Home</a>
    <a href="/subjects/add">Add Data Subject</a>
    <table border="1"="width:25%">
      <thead>
        <tr>
          <th>ID</th>
          <th>Subject Name</th>
          <th>Teacher</th>
          <th>Actions</th>
        </tr>
      </thead>
      <tbody>
        <%dataSubject.forEach(data =>{ %>
          <tr>
            <td><%=data.id%></td>
            <td><%=data.subject_name%></td>
            <td><%=data.teacher_name%></td>
            <td>
              <a href="/subjects/<%= data.id %>/enrolledstudents">Enrolled Students</a>
            </td>
          </tr>
        <%})%>
      </tbody>
   </table>
  </body>
</html>
