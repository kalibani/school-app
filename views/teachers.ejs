<!DOCTYPE html>
<html>
  <%- include partial/head %>
  <body>
    <h1>Halaman Teachers</h1>
    <a href="/teachers/add">Teacher Add</a>
    <a href="/">Back to Home</a>
    <table border="1"="width:25%">
      <thead>
        <tr>
          <th>ID</th>
          <th>First Name</th>
          <th>Last Name</th>
          <th>Email</th>
          <th>Subject Name</th>
          <th>Action</th>
        </tr>
      </thead>
      <tbody>
        <%dataTeachers.forEach(data =>{ %>
          <tr>
            <td><%=data.id%></td>
            <td><%=data.first_name%></td>
            <td><%=data.last_name%></td>
            <td><%=data.email%></td>
            <td><%=data.subject_name%></td>
            <td>
              <a href="teachers/edit/<%=data.id%>">Edit</a>
              <a href="teachers/delete/<%=data.id%>">Delete</a>
            </td>
          </tr>
        <%})%>
      </tbody>
   </table>
  </body>
</html>
